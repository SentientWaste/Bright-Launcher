<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:vm="using:Bright_Launcher.ViewModels.Pages.Setting"
             x:Class="Bright_Launcher.Views.Pages.Setting.SettingPage"
			 x:DataType="vm:SettingPageViewModel">
	
	<Grid RowDefinitions="1*, 80">
		<Border CornerRadius="10" Margin="20 20 20 20" BoxShadow="0 2 20 0 #66000000" Background="#DBDBDB" Grid.Row="0">
			<ContentControl Content="{Binding Content}"/>
		</Border>
		
		<Border BoxShadow="0 2 20 0 #66000000" Background="#DBDBDB" Grid.Row="1">
			
		</Border>
	</Grid>
</UserControl>
